{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules",
    ".tmp",
    "test/",
    "assets/"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "api/",
    "config/"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js json coffee"
}